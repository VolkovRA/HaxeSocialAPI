package social.target.ok.enums;

/**
 * Коды ошибок OK.  
 * Если сервер не сможет обработать запрос, заголовок HTTP
 * `invocation-error` будет содержать код ошибки. Кроме того,
 * ответ содержит подробную информацию в формате JSON.
 * @see Документация: https://apiok.ru/dev/errors
 */
@:enum abstract ErrorCode(Int) to Int from Int
{
    /**
     * Неизвестная ошибка.
     */
    var UNKNOWN = 1;

    /**
     * Отказ в разрешении.  
     * Возможная причина - пользователь не авторизовал приложение
     * на выполнение операции.
     */
    var PERMISSION_DENIED = 10;

    /**
     * Отсутствующий или неверный параметр.
     */
    var PARAM = 100;

    /**
     * Ошибка возвращена сервером приложений для уведомления о
     * неверной информации транзакции.
     */
    var CALLBACK_INVALID_PAYMENT = 1001;

    /**
     * Неверная платежная транзакция.
     */
    var INVALID_PAYMENT = 1003;

    /**
     * Слишком частые запросы платежа.
     */
    var DUPLICATE_PAYMENT = 1004;

    /**
     * У пользователя недостаточно денег на аккаунте.
     */
    var NOT_ENOUGH_MONEY = 1005;

    /**
     * Параметр `application_key` не указан или указан неверно.
     */
    var PARAM_API_KEY = 101;

    /**
     * Истек срок действия ключа сессии.
     */
    var PARAM_SESSION_EXPIRED = 102;

    /**
     * Неверный ключ сессии.
     */
    var PARAM_SESSION_KEY = 103;

    /**
     * Неверная подпись.
     */
    var PARAM_SIGNATURE = 104;

    /**
     * Неверная повторная подпись.
     */
    var PARAM_RESIGNATURE = 105;

    /**
     * Неверный идентификатор дискуссии.
     */
    var PARAM_ENTITY_ID = 106;

    /**
     * Достигнут предел вызовов метода.
     */
    var LIMIT_REACHED = 11;

    /**
     * Неверный идентификатор пользователя.
     */
    var PARAM_USER_ID = 110;

    /**
     * Видео-чат отключен.
     */
    var VCHAT_SERVICE_DISABLED = 1101;

    /**
     * Пользователь недоступен для видео-чата или видео-сообщения.
     */
    var TARGET_USER_UNAVAILABLE = 1102;

    /**
     * Указанный пользователь должен быть другом.
     */
    var FRIENDSHIP_REQUIRED = 1103;

    /**
     * Операция прервана пользователем.
     */
    var CANCELLED = 12;

    /**
     * Неверный идентификатор альбома.
     */
    var PARAM_ALBUM_ID = 120;

    /**
     * Ошибка вызова батчинга.
     */
    var BATCH = 1200;

    /**
     * Неверный идентификатор фотографии.
     */
    var PARAM_PHOTO_ID = 121;

    /**
     * Неверный идентификатор виджета.
     */
    var PARAM_WIDGET = 130;

    /**
     * Платформы для приложения не установлены.
     */
    var APP_NO_PLATFORM_ALLOWED = 1300;

    /**
     * Указанное устройство не доступно.
     */
    var APP_DEVICE_NOT_ALLOWED = 1301;

    /**
     * Устройство не указано.
     */
    var APP_DEVICE_NOT_SPECIFIED = 1302;

    /**
     * Неверный идентификатор сообщения.
     */
    var PARAM_MESSAGE_ID = 140;

    /**
     * Ошибка поиска мест.
     */
    var APP_EMPTY_SEARCH_PARAMS = 1400;

    /**
     * Ошибка поиска мест.
     */
    var APP_SEARCH_SCENARIO_DOES_NOT_EXIST = 1401;

    /**
     * Неверный идентификатор комментария.
     */
    var PARAM_COMMENT_ID = 141;

    /**
     * Неверный идентификатор события.
     */
    var PARAM_HAPPENING_ID = 150;

    /**
     * Неверный идентификатор фотографии события.
     */
    var PARAM_HAPPENING_PHOTO_ID = 151;

    /**
     * Неверный идентификатор группы.
     */
    var PARAM_GROUP_ID = 160;

    /**
     * Сервис временно недоступен.
     */
    var SERVICE = 2;

    /**
     * Приложение не может выполнить операцию.  
     * В большинстве случаев причиной является попытка получения
     * доступа к операции без авторизации от пользователя.
     */
    var PARAM_PERMISSION = 200;

    /**
     * Неверный тип POST-контента (Graph API).  
     * Необходимо добавить заголовок
     * `Content-Type: application/json;charset=utf-8`
     */
    var GRAPH_PARAM_REQUEST = 2001;

    /**
     * Не multi-part запрос при добавлении фотографий.
     */
    var NOT_MULTIPART = 21;

    /**
     * Приложение отключено.
     */
    var PARAM_APPLICATION_DISABLED = 210;

    /**
     * Неверный идентификатор выбора.
     */
    var PARAM_DECISION = 211;

    /**
     * Неверный идентификатор значка.
     */
    var PARAM_BADGE_ID = 212;

    /**
     * Неверный идентификатор подарка.
     */
    var PARAM_PRESENT_ID = 213;

    /**
     * Неверный идентификатор типа связи.
     */
    var PARAM_RELATION_TYPE = 214;

    /**
     * Пользователь должен активировать свой аккаунт.
     */
    var NOT_ACTIVATED = 22;

    /**
     * Неверный формат поля `fields`
     */
    var PARAM_FIELDSET = 220;

    /**
     * Пользователь не вовлечён в приложение.
     */
    var NOT_YET_INVOLVED = 23;

    /**
     * Пользователь не является владельцем объекта.
     */
    var NOT_OWNER = 24;

    /**
     * Ошибка рассылки нотификаций.  
     * Пользователь неактивен в приложении.
     */
    var NOT_ACTIVE = 25;

    /**
     * Ошибка рассылки нотификаций.  
     * Достигнут лимит нотификаций для приложения.
     */
